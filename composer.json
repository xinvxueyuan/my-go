    "require": {
        "php": "^8.4",
        "dompdf/dompdf": "^2.0",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.6",
        "slim/php-view": "^3.2",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
